namespace TestClass {
    def void main(String[] args) {
        for (int i = 0; i < 10; i = i + 1) {
            if (i == 5) {
                continue;
            } elif (i == 8) {
                break;
            }
            print(i);
            print(" ");
        }
        print("\n");
        int i = 10;
        while (i >= 0) {
            i = i - 1;
            if (i == 3) {
                continue;
            } elif (i == 2) {
                break;
            }
            print(i);
            print(" ");
        }
    }
}
